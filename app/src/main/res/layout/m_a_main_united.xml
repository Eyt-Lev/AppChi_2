<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    android:layout_marginStart="2dp"
    tools:context=".main.Main">

<LinearLayout android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:textAllCaps="false"
            android:textColor="#424242"
            android:textSize="14sp"
            app:icon="@drawable/ic_baseline_checklist_24"
            app:iconGravity="textStart"
            app:iconSize="40dp"
            app:iconTint="#424242"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button310"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:textAllCaps="false"
            android:textColor="#424242"
            android:textSize="14sp"
            app:icon="@drawable/ic_baseline_translate_24"
            app:iconGravity="textStart"
            app:iconSize="40dp"
            app:iconTint="#424242"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck" />

        <Button
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:id="@+id/button245"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:isScrollContainer="true"
            android:textAllCaps="false"
            android:textColor="#424242"
            app:iconGravity="textStart"
            android:textSize="14sp"
            app:icon="@drawable/ic_baseline_mic_24"
            app:iconSize="40dp"
            app:iconTint="#424242" />

        <Button
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:id="@+id/button235"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:isScrollContainer="true"
            android:textAllCaps="false"
            android:textColor="#424242"
            app:iconGravity="textStart"
            android:textSize="14sp"
            app:icon="@drawable/ic_baseline_flashlight_on_24"
            app:iconSize="40dp"
            app:iconTint="#4B463F" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:id="@+id/button221"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:isScrollContainer="true"
            android:textAllCaps="false"
            android:textColor="#424242"
            android:textSize="14sp"
            app:icon="@drawable/ic_baseline_send_24"
            app:iconGravity="textStart"
            app:iconSize="40dp"
            app:iconTint="#424242" />

        <Button
            android:id="@+id/button233"
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:isScrollContainer="true"
            android:textAllCaps="false"
            android:textColor="#424242"
            app:iconGravity="textStart"
            android:textSize="14sp"
            app:icon="@drawable/ic_baseline_music_note_24"
            app:iconSize="40dp"
            app:iconTint="#424242"
            app:iconTintMode="multiply"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button642"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:isScrollContainer="true"
            android:textAllCaps="false"
            android:textColor="#424242"
            android:textSize="14sp"
            app:icon="@drawable/ic_baseline_calculate_24"
            app:iconGravity="textStart"
            app:iconSize="40dp"
            app:iconTint="#424242"
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck" />

        <Button
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:id="@+id/button6"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:isScrollContainer="true"
            android:textAllCaps="false"
            android:textColor="#424242"
            app:iconGravity="textStart"
            android:textSize="14sp"
            app:icon="@drawable/dice12"
            app:iconSize="40dp"
            app:iconTint="#424242"
            app:iconTintMode="multiply"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:id="@+id/button343"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:textAllCaps="false"
            android:textColor="#424242"
            android:textSize="14sp"
            app:iconGravity="textStart"
            app:icon="@drawable/ic_baseline_cruelty_free_24"
            app:iconSize="40dp"
            app:iconTint="#4B463F" />

        <Button
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:id="@+id/button23"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:textAllCaps="false"
            android:textColor="#424242"
            android:textSize="14sp"
            app:iconGravity="textStart"
            app:icon="@drawable/ic_baseline_change_history_24"
            app:iconSize="40dp"
            app:iconTint="#4B463F" />

        <Button
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:id="@+id/button454"
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:textAllCaps="false"
            android:textColor="#424242"
            android:textSize="14sp"
            app:iconGravity="textStart"
            app:icon="@drawable/ic_baseline_auto_graph_24"
            app:iconSize="40dp"
            app:iconTint="#424242"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            tools:ignore="ButtonStyle,SpeakableTextPresentCheck"
            android:id="@+id/button347"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:layout_marginHorizontal="4dp"
            android:layout_weight="1"
            android:textAllCaps="false"
            android:textColor="#424242"
            android:textSize="14sp"
            app:iconGravity="textStart"
            app:icon="@drawable/ic_baseline_local_gas_station_24"
            app:iconSize="40dp"
            app:iconTint="#4B463F" />
    </LinearLayout>
</LinearLayout>
    <com.google.android.material.navigation.NavigationView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:id="@+id/navView"
        app:menu="@menu/drawer" />
</androidx.drawerlayout.widget.DrawerLayout>
